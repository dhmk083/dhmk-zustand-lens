  },
  "peerDependencies": {
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "immer": "^9.0.12",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4",
    "zustand": "^4.0.0-rc.1"
  },
  "dependencies": {
    "@dhmk/utils": "^2.4.2"
  },
  "jest": {
    "preset": "ts-jest"
  }
