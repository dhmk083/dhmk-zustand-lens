{
  "name": "@dhmk/zustand-lens",
  "version": "2.0.2",
  "description": "Lens support for zustand",
  "keywords": [
    "zustand",
    "middleware",
    "lens"
  ],
  "license": "MIT",
  "repository": "github:dhmk083/dhmk-zustand-lens",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
